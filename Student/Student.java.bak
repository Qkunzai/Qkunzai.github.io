public class Student 
{
	String name; //姓名
	int age; //年龄
	String sex;//性别
	String zy;//专业
	String ah;//爱好

	public Student(String name,int age,String sex,String zy,String ah){
		this.name=name;
		this.age=age;
		this.sex=sex;
		this.zy=zy;
		this.ah=ah;
	}
	public void say(){
		System.out.println("姓名："+name+"年龄:"+age+"性别:"+sex+"专业:"+zy+"爱好:"+ah);
	}
}