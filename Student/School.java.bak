public class School
{
	String sname;//学校名称
	String dz;//学校地址
	String zd;//占地面积
	int rs;//在校人数
	public School(String sname,String dz,String zd,int rs){
		this.sname=sname;
		this.dz=dz;
		this.zd=zd;
		this.rs=rs;
	}
	public void dc(){
		System.out.println("学校名称"+sname+"\t学校地址"+dz+"\t占地面积"+zd+"\t在校人数"+rs);
	}
}