public class Huo
{
	String Hname;//火车名
	String time;//发车时间
	double price;//车票价格
	String action;//始发站
	String over;//终点站


	public Huo(String Hname,String time,double price,String action,String over){
		this.Hname=Hname;
		this.time=time;
		this.price=price;
		this.action=action;
		this.over=over;
	}
	public void lcb(){
		System.out.println("列次:"+Hname+"\t发车时间:"+time+"\t车票价格:"+price+"\t始发站:"+action+"\t终点站:"+over);
	}
}