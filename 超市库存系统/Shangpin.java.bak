			this.type=type;
	}
	public void say(){
		System.out.println("商品名称"+sname+"\t商品价格"+price+"\t商品数量"+number+"\t入库时间"+time+"\t商品类型"+type);
	}
	public void say1(){
		System.out.println("商品名称"+sname+"\t商品价格"+price+"\t商品数量"+number+"\t出库时间"+time+"\t商品类型"+type);
	}
	public void sayNumber(){
		System.out.println("类型"+type);
	}
}
